<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="RPO_college.ToDo"
             xmlns:pages="clr-namespace:RPO_college">
  <FlyoutPage.Flyout>
    <pages:ToDoFlyout x:Name="FlyoutPage" />
  </FlyoutPage.Flyout>
  <FlyoutPage.Detail>
    <NavigationPage>
      <x:Arguments>
        <pages:ToDoDetail />
      </x:Arguments>
    </NavigationPage>
  </FlyoutPage.Detail>
</FlyoutPage>